package pkg.Shape;

public sealed class Circle extends Shape permits pkgShape.Ellipse(double test){

          private double radius();
          public double area();
          
          
          protected double setRadius(double num){
              this.radius = num;
          }
          
          protected double getRadius(){
              return this.radius;
          }
          

          if (getRadius() < 0){
              throw new IllegalArgumentException();
          }
          
}




assertEquals(314.15,c1Area, 0.01);
