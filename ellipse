package pkgShape;

public sealed class Ellipse extends circle permits pkgShape.Ellipsoid(double num1, double num2){

          private double minorRadius();
          public boolean isCircle();
          
          public void setMinorRadius(double yes){
              this.minorRadius = yes;
          }
          
          public double getMinorRadius(){
              return this.minorRadius;
          }
          
          
          if(num1 < 0){
              throw IllegalArgumentException();
          }
          
          if(num2 < 0){
              throw IllegalArgumentException();
          }
          
          
}
